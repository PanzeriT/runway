package main

import (
	"{{ .ModuleName }}/internal/server/admin"
	"{{ .ModuleName }}/internal/server/admin/template/page"
	"{{ .ModuleName }}/internal/server/admin/template/data"
	"net/http"
	
	"github.com/labstack/echo/v4"
)

func main() {
	e := echo.New()
  
  admin.CreateRoutes(e)

	e.GET("/", func(c echo.Context) error {
		return admin.Render(c, http.StatusOK, page.Intro("TITLE", 0, &data.User{}))
	})
	e.Logger.Fatal(e.Start(":1323"))
}
{{ end }}
